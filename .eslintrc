{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },

    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],

    "rules": {
        "no-console": "off"
    },

    "env": {
        "browser": true,
        "node": true
    },

    "plugins": [
        "react"
    ]
}